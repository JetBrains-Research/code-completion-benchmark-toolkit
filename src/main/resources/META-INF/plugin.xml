<idea-plugin url="https://github.com/ml-in-programming/code-completion-benchmark-toolkit">
    <id>code-completion-benchmark-toolkit</id>
    <name>code-completion-benchmark-toolkit</name>
    <vendor url="https://research.jetbrains.org/ru/groups/ml_methods">Machine Learning Methods in Software Engineering</vendor>

    <description><![CDATA[
    IntelliJ code completion benchmark toolkit
    ]]></description>

    <depends>com.intellij.modules.lang</depends>

    <extensionPoints>
        <extensionPoint qualifiedName="code_completion_benchmark.toolkit.providers.modelRunnerProvider"
                        interface="org.jetbrains.research.groups.ml_methods.code_completion_benchmark.toolkit.providers.ModelRunnerProvider"/>
    </extensionPoints>

    <extensions defaultExtensionNs="com.intellij">
        <applicationService serviceImplementation="org.jetbrains.research.groups.ml_methods.code_completion_benchmark.toolkit.services.ModelRunnerRegistrar"/>
    </extensions>
</idea-plugin>